<div class="sidebar">
  <div class="sidebar-inner">
    <ul class="main-menu">
      <%- partial('partials/main_menu', { context: 'sidebar' }) %>
    </ul>
    <div class="list">
      <% if (type !== 'search') { %>
        <h2>
          <% titles = {
            api: 'API',
            integrations: 'Integrations',
            contributed: 'Contributed',
            examples: 'Examples',
            tutorials: 'Tutorials',
            cookbook: 'Cookbook<sup class="alpha">alpha</sup>',
            'best-practices': 'Best practices',
              testing: 'Testing',
          } %>
          <%- titles[type] %>
          <% if (['cookbook', 'best-practices', 'contributed', 'testing', 'integrations'].indexOf(type) === -1) { %>
            <select class="version-select">
              <option value="SELF" selected>1.0</option>
            </select>
          <% } %>
        </h2>
        <%- partial('partials/toc', { type: type }) %>
      <% } %>
    </div>
  </div>
</div>

